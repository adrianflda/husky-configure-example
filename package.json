{
  "name": "husky-configure-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:adrianflda/husky-configure-example.git",
  "author": "Adrian <adrian.flda@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "hooks:pre-commit": "node ./hooks/pre-commit.js",
    "hooks:pre-push": "node ./hooks/pre-push.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn run hooks:pre-commit",
      "pre-push": "yarn test && yarn run hooks:pre-push"
    }
  },
  "dependencies": {
    "husky": "^4.3.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "jest": "^26.4.2",
    "pretty-format": "^26.4.2"
  }
}
